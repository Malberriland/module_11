import builtins


def introspection_info(obj):
    obj_type = type(obj).__name__
    meth = []
    attrs = []
    att = dir(obj)

    def methods():
        return methods.__dir__()

    for j in att:
        if j in methods.__dir__():
            meth.append(j)
        else:
            attrs.append(j)

    module = obj.__class__.__module__

    dictionary = {'type': obj_type, 'attributes': attrs, 'methods': methods(), 'module': module}
    return dictionary


number_info = introspection_info(42)
print(number_info)
