import builtins


def introspection_info(obj):
    obj_type = type(obj).__name__
    meth = []
    attrs = []
    att = dir(obj)
    for j in att:
        if hasattr(builtins, j):
            attrs.append(j)
        else:
            meth.append(j)

    module = obj.__class__.__module__

    dictionary = {'type': obj_type, 'attributes': attrs, 'methods': meth, 'module': module}
    return dictionary


number_info = introspection_info(42)
print(number_info)
